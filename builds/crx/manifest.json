{
  "name": "OneeChan",
  "version": "5.0.17",
  "manifest_version": 2,
  "description": "Customizable rice and themes for 4chan and 4chan X.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [{
    "js": ["script.js"],
    "matches": ["*://www.4chan.org/","*://boards.4chan.org/*","*://sys.4chan.org/*"],
    "all_frames": true,
    "run_at": "document_start"
  }],
  "homepage_url": "http://seaweedchan.github.io/OneeChan/",
  "minimum_chrome_version": "26",
  "permissions": [
    "storage"
  ]
}
